 {
    "manifest_version": 2,         
    "name": "Trial",
    "version": "1.0",
    "description": "Pop up alert and name",
    "icons": {
        "128": "ext-icon.png"
    },
    "browser_action": {
        "default_icon": "ext-icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "<popup.html>"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
    }
