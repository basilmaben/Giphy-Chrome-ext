{
    "manifest_version": 2,
    "name": "Example",
    "description": "Displaying Notepad",
    "version": "1.0",
    "content_scripts":[{
        "matches":["<all_urls"],
        "js": ["content.js"]
    }],
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs"
    ]
}